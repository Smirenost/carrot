{
  "name": "@liquid-carrot/neat",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test:node && npm run test:browser",
    "test:node": "./node_modules/.bin/mocha test/node.js",
    "test:browser": "npm run build:test && live-server test/",
    "build": "npm run build:src && npm run doc",
    "build:src": "./node_modules/.bin/webpack",
    "build:test": "./node_modules/.bin/webpack test/node.js -o test/browser.js",
    "doc": "npm run doc:markdown && npm run doc:html",
    "doc:html": "./node_modules/.bin/jsdoc -c jsdoc.conf.json",
    "doc:markdown": "./node_modules/.bin/jsdoc2md src/*.js > DOCUMENTATION.md",
    "serve": "sudo",
    "serve:doc": "sudo",
    "serve:repo": "./node_modules/.bin/live-server"
  },
  "author": "Liquid Carrot <people@liquidcarrot.io> (https://liquidcarrot.io)",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "jsdoc-to-markdown": "^5.0.0",
    "live-server": "^1.2.1",
    "mocha": "^6.2.0",
    "perf_hooks": "0.0.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "vis-network": "^6.4.0"
  }
}
