{
  "name": "@liquid-carrot/carrot",
  "version": "0.3.18",
  "description": "A Simple Node.js AI Library for Neural Network",
  "main": "dist/production/index.min.js",
  "module": "dist/production/index.min.js",
  "unpkg": "dist/production/index.browser.min.js",
  "jsdelivr": "dist/production/index.browser.min.js",
  "browser": {
    "fs": false,
    "child_process": false
  },
  "scripts": {
    "build:src": "tsc && node scripts/src.build.node.js && node scripts/src.build.browser.js && node scripts/src.build.browser.production.js && node scripts/src.build.node.production.js",
    "deploy:docs": "tsc && node theme/deploy-docs.js",
    "test": "./node_modules/.bin/nyc --reporter=html --reporter=text node ./node_modules/mocha/bin/mocha --recursive build/test/",
    "lint": "tslint --project ./",
    "test:debug": "./node_modules/.bin/nyc --reporter=html --reporter=text node ./node_modules/mocha/bin/mocha inspect --recursive test/ --exclude test/unit.js",
    "test:forever": "./node_modules/.bin/nodemon ./node_modules/.bin/mocha --reporter spec test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liquidcarrot/carrot.git"
  },
  "keywords": [
    "Artificial Intelligence",
    "AI",
    "Deep Learning",
    "DL",
    "Machine Learning",
    "ML",
    "Neural Network",
    "Neural Networks",
    "NN",
    "Convolutional Neural Network",
    "Convolutional Neural Networks",
    "Convolutional NN",
    "Convolutional NNs",
    "CNN",
    "Artificial Neural Network",
    "Artificial Neural Networks",
    "Artificial NN",
    "Artificial NNs",
    "ANN",
    "Recurrent Neural Network",
    "Recurrent Neural Networks",
    "Recurrent NN",
    "Recurrent NNs",
    "RNN",
    "Perceptron",
    "Sigmoid",
    "TanH",
    "ArcTan"
  ],
  "author": "Liquid Carrot <people@liquidcarrot.io> (https://liquidcarrot.io)",
  "contributors": [
    "Luis Ernesto Carbonell <luis@liquidcarrot.io>",
    "Christian George Echevarria <chris@liquidcarrot.io>"
  ],
  "files": [
    "/src",
    "/test",
    "/dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liquidcarrot/carrot/issues"
  },
  "homepage": "https://liquidcarrot.io/",
  "dependencies": {
    "@types/parcel-bundler": "^1.12.1",
    "lodash": "^4.17.15",
    "riteway": "^6.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "parcel-bundler": "^1.12.4",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "include": [
      "build/src/architecture/*.js",
      "build/src/methods/*.js",
      "build/src/NEAT.js"
    ],
    "exclude": [
      "test"
    ]
  }
}
